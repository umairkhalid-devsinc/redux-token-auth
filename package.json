  "author": "Kyle Corbelli",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/react": "^18.2.55",
    "@types/react-redux": "^7.1.33",
    "axios": "^1.6.7",
    "react": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1"
  }
